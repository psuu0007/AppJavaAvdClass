<script type="text/javascript">

	var num = 1;
	
	var str = '1';
	
	document.write(num == str);   //값 비교
	document.write(num === str);  //값 비교 타입 비교
	
	document.write(num != str);   //값 비교
	document.write(num !== str);  //값 비교 타입 비교
	
</script>

